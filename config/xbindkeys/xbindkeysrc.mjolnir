# Bind screen locker.
"xscreensaver-command --lock"
  m:0x8 + c:49
  Alt + grave

# Toggle touchpad.
"xinput-toggle -r Touchpad -n"
  m:0x18 + c:70
  Alt+Mod2 + F4

# Control backlight.
"xbacklight -dec 100"
  m:0x0 + c:72
  XF86MonBrightnessDown

"xbacklight -inc 100"
  m:0x0 + c:73
  XF86MonBrightnessUp
