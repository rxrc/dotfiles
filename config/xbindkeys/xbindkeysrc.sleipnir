# Bind screen locker.
"loginctl lock-session"
  m:0x50 + c:49
  Mod2+Mod4 + grave

# Tab commands on mouse left and right scroll.
"xte 'keydown Control_L' 'keydown Shift_L' 'key Tab' 'keyup Control_L' 'keyup Shift_L'"
  b:6

"xte 'keydown Control_L' 'key Tab' 'keyup Control_L'"
  b:7

"flameshot gui"
  m:0x50 + c:107
  Mod2+Mod4 + Print
